[init]
    defaultBranch = main
[user]
    email = pinedap.angel@gmail.com
    name = UserIsntAvailable
[credential "https://github.com"]
    helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
    helper = !/usr/bin/gh auth git-credential
[alias]
    a = add
    au = add --update
    ap = add --patch
    b = branch
    bd = branch --delete --force
    c = commit
    ca = commit --amend
    cm = commit --message
    cam = commit --amend --message
    can = commit --amend --no-edit
    co = checkout
    cob = checkout -b
    d = diff
    dc = diff --cached
    m = merge
    ms = merge --squash
    ps = push
    psu = push --set-upstream
    psf = push --force
    pl = pull
    plf = pull --force
    r = restore
    rs = restore --staged
    rb = rebase
    rbi = rebase --interactive
    s = status --short
    st = stash
    sts = stash --staged
    stl = stash list
    stc = stash clear
    std = stash drop
    stp = stash pop
    l = log --oneline
    ll = log --format=fuller
    lg = log --all --graph --pretty=format:'%C(auto)%h  %an  %as %d%n%s%n'
    last = show --format=fuller
[core]
    autocrlf = false
[commit]
    verbose = true
[diff]
    renames = copies
[status]
    showUntrackedFiles = all
[tag]
    sort = -taggerdate
[url "https://github.com/"]
    insteadOf = "gh:"
[url "https://gitlab.com/"]
    insteadOf = "gl:"
[url "https://bitbucket.com/"]
    insteadOf = "bb:"
